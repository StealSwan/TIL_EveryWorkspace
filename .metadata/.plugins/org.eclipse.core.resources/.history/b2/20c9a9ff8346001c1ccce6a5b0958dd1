package spring.di.ex7;

import java.util.ArrayList;

public class Sawon {

	private String name;
	private int pay;
	private String jikgup;
	private ArrayList<String> buseo;
	
	
	//name, jikgup 생성자 주입
	public Sawon(String name, String jikgup) {
		super();
		this.name = name;
		this.jikgup = jikgup;
	}
	
	
}
