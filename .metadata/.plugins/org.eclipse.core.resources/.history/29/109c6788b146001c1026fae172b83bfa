public class HomeController {

	//@RequestMapping(value = "/", method = RequestMethod.GET)
	@GetMapping("/")
	public String Hello() {
		
		//model: request
		
		return "result1";	//포워드/WEB-INF/day1116/result1.jsp
	}
}
