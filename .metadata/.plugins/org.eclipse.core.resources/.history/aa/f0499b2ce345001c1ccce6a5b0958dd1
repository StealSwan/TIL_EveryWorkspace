package spring.di.ex2;

public class MyInfo {

	String name;
	int age;
	String addr;
	
	
	//»ý¼ºÀÚ
	public MyInfo(String name, int age, String addr) {
		super();
		this.name=name;
		this.age=age;
		this.addr=addr;
	}
	
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return super.toString();
	}
}
