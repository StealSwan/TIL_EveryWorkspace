package spring.di.quiz;

public class Mydata {
	
	//맴버변수 선언
	Score score;
	Info info;
	
	
	//위의 두개를 주입받을 수 있는 생성자	
	public Mydata(Score score, Info info) {
		super();
		this.score = score;
		this.info = info;
	}

	
	//출력 메서드
	public void writeData() {
		System.out.println("이름: " + info.getName());
		System.out.println("나이: " + info.getAge());
		System.out.println("국어점수: " + score.getKor());
		System.out.println("영어점수: " + score.getEng());
		System.out.println("수학점수: " + score.getMat());
	}
}
