package spring.di.quiz;

public class Score {

	private int kor, eng, mat;

	
	//»ý¼ºÀÚ
	public Score1(int kor, int eng, int mat) {
		super();
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
	}


	public int getKor() {
		return kor;
	}

	
	//setter, getter
	public void setKor(int kor) {
		this.kor = kor;
	}


	public int getEng() {
		return eng;
	}


	public void setEng(int eng) {
		this.eng = eng;
	}


	public int getMat() {
		return mat;
	}


	public void setMat(int mat) {
		this.mat = mat;
	}
	
	

	
	
	
}
