package spring.di.ex7;

import java.util.ArrayList;

public class Sawon {

	private String name;
	private int pay;
	private String jikgup;
	private ArrayList<String> buseo;
	
	
	//name, jikgup 생성자 주입
	public Sawon(String name, String jikgup) {
		super();
		this.name = name;
		this.jikgup = jikgup;
	}

	//setter
	public void setName(String name) {
		this.name = name;
	}

	//setter
	public void setBuseo(ArrayList<String> buseo) {
		this.buseo = buseo;
	}

	
	//4개 getter	
	public String getName() {
		return name;
	}

	public int getPay() {
		return pay;
	}

	public String getJikgup() {
		return jikgup;
	}

	public ArrayList<String> getBuseo() {
		return buseo;
	}
	
	
	
}
