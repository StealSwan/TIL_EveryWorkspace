package spring.di.quiz;

public class Info {

	private String name;
	private int age;
	
	//기본 생성자 - 없으면 에러
	public Info() {
		// TODO Auto-generated constructor stub
	}
	
	
	//생성자
	public Info(String name, int age) {
		this.name = name;
		this.age = age;
	}

	
	//setter, getter	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	

}
