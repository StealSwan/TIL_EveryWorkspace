package spring.di.ex7;

public class Emp {
	
	Sawon sawon;
	String empName;	//회사명
	String empLoc; //위치
	
	//생성자
	public Emp(String empName) {
		super();
		this.empName = empName;
	}

	
	//setter	
	public void setSawon(Sawon sawon) {
		this.sawon = sawon;
	}

	public void setEmpLoc(String empLoc) {
		this.empLoc = empLoc;
	}

	
	//정보출력
	public void writeEmp() {
		System.out.println("사원명: " + sawon.getName());
		System.out.println("급여: " + sawon.getPay());
		System.out.println("직급: " + sawon.getJikgup());
	}
	
}
