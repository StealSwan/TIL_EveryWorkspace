package day0806;

public class GuguGaro_07 {

	public static void main(String[] args) {
		
		System.out.println("** 가로 방향 구구단 출력 **");
		
		
		for (int i = 2; i < 10; i++) {
			System.out.print("[" + i +"단]");
		}
		
		System.out.println("");
		
		for (int i = 2; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
				System.out.println(i + "x" + j + "=" + i*j);
				continue;
			}
		}
	}
}
