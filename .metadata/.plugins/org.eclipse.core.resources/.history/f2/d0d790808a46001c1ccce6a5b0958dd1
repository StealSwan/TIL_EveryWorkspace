package spring.di.quiz;

public class Score {

	private int kor, eng, mat;

	
	//»ý¼ºÀÚ
	public Score(int kor, int eng, int mat) {
		super();
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
	}

	
	//setter
	public void setKor(int kor) {
		this.kor = kor;
	}


	public void setEng(int eng) {
		this.eng = eng;
	}


	public void setMat(int mat) {
		this.mat = mat;
	}
	

	
	
	
	
}
