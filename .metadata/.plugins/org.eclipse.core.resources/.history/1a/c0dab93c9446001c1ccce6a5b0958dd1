package spring.di.quiz;

public class Info {

	private String name;
	private int age;
	
	public Info() {
		// TODO Auto-generated constructor stub
	}
	
	//»ý¼ºÀÚ
	public Info(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}

	
	//setter, getter	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	

}
