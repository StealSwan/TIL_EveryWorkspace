package spring.di.ex5;

public class Jumun {

	Shop shop;
	int cnt;
	String name;
	
	
	//name 생성자
	public Shop(String sangpum) {
		this.sangpum = sangpum;
	}
	
	
	//shop, cnt setter
	public void setShop(Shop shop) {
		this.shop = shop;
	}

	public void setCnt(int cnt) {
		this.cnt = cnt;
	}
	
	//출력문
	public void WriteJumun() {
		System.out.println("주문자: " + name);
		System.out.println("갯수: " + cnt);
		System.out.println("***상품정보***");
		shop.writeShope();
	}
	
	
	
	
	
}
