package spring.di.ex5;

public class Shop {

	private String sangpum;
	private int price;
	private String color;
	
	//sangpum 생성자
	public Shop(String sangpum) {
		this.sangpum = sangpum;
	}
	
	
	//price, color	
	public void setPrice(int price) {
		this.price = price;
	}
	public void setColor(String color) {
		this.color = color;
	}
	
	
	//출력
	public void writeShope() {
		System.out.println("");
		System.out.println("");
		System.out.println("");
	}

	
	
}
